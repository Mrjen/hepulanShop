.swipe-item-wrapper{height:90rpx;position:relative;overflow:hidden;padding:0 10px;margin-top:10rpx}.swipe-item-wrapper .swipe-actions{display:flex;flex-direction:row;justify-content:center;align-items:center;height:100%;width:140rpx;position:absolute;top:0;z-index:1}.swipe-item-wrapper .swipe-actions.actions-left{left:0}.swipe-item-wrapper .swipe-actions.actions-right{right:0}.swipe-item-wrapper .swipe-content{width:100%;height:100%;line-height:90rpx;box-sizing:border-box;position:absolute;top:0;left:0;z-index:5;padding:0 18rpx;border-top:1px solid #e5e5e5;border-bottom:1px solid #e5e5e5;background-color:white}.swipe-item-wrapper .swipe-content .swipe-title{white-space:nowrap;overflow:hidden;text-overflow:ellipsis}.swipe-item-wrapper .swipe-btn{flex:1;height:90rpx;line-height:90rpx;text-align:center;color:white}.swipe-item-wrapper .swipe-btn.del{background-color:#f4333c}.swipe-item-wrapper .swipe-btn.edit{background-color:#108ee9}