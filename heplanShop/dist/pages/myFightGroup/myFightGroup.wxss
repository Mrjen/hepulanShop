page{background:#f7f7f7}.page_nav{width:100%;height:90rpx;background:#fff;display:flex;margin-bottom:20rpx}.page_nav view{flex:1;text-align:center;line-height:90rpx}.page_nav .active{border-bottom:2px solid #16382e}.list .list_item{width:100%;background:#fff}.list .list_item .item_nav{width:100%;height:70rpx;line-height:70rpx;padding:0 20rpx;position:relative;font-size:26rpx}.list .list_item .item_main{display:flex;padding:20rpx;justify-content:space-between}.list .list_item .item_main image{width:152rpx;height:152rpx;display:block}.list .list_item .item_main .item_info{flex:1;padding-left:20rpx}.list .list_item .item_main .item_info view:nth-of-type(1){margin-bottom:10rpx}.list .list_item .item_main .item_info view:nth-of-type(2){margin-bottom:10rpx;font-size:26rpx;text-decoration:line-through}.list .list_item .item_main .item_info view:nth-of-type(3){margin-bottom:10rpx;font-size:26rpx}.list .list_item .item_total{text-align:right;padding:0 20rpx 20rpx 0;font-size:26rpx;position:relative}.list .list_item .item_total text{color:red}.list .list_item .item_bottom_nav{display:flex;justify-content:flex-end;padding:0 20rpx;height:80rpx;align-items:center}.list .list_item .item_bottom_nav navigator{margin-left:30rpx;font-size:24rpx;display:inline-block;padding:8rpx 20rpx;border-radius:4rpx}.list .list_item .item_bottom_nav navigator:nth-of-type(1){border:1px solid #17382f;color:#17382f}.list .list_item .item_bottom_nav navigator:nth-of-type(2){background:#17382f;color:#fff}